	@FindBy(id="dep_date_0")
	WebElement valueDepartureDate;
	
	@FindBy(id="no_days_0")
	WebElement valueNoOfDays;
	
	@FindBy(id="username_show")
	WebElement loginSuccessTxt;
	
	@FindBy(id="username_show")
	WebElement loginSuccessTxt;
	
	@FindBy(id="username_show")
	WebElement loginSuccessTxt;
	
